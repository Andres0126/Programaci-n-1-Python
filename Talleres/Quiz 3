n=float(input("Ingrese un valor: "))
m=float(input("Ingrese otro valor: "))


if n >= m:
    print("ingrese en el siguiente orden n < m")
else:
    if n%2==0:
        x = 0
        while x <= m:
            n = n+2
            x=n
            print(x)
    else:
        x = 0
        while x <= m:
            n = n+1
            n = n+2
            x=n
            print(x)